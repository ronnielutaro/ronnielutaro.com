{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "screenName": "Homepage",
  "screenId": "home-001",
  "version": "1.0.0",
  "designSystem": "Liquid Glass + MUI",
  "viewport": {
    "mobile": "375x812",
    "tablet": "768x1024",
    "desktop": "1440x900"
  },
  "layout": {
    "type": "full-page-scroll",
    "sections": [
      {
        "id": "navigation",
        "component": "AppBar",
        "position": "fixed-top",
        "muiComponent": "AppBar",
        "styling": {
          "background": "glassmorphic-blur",
          "backdropFilter": "blur(20px)",
          "backgroundColor": "rgba(255, 255, 255, 0.7)",
          "borderBottom": "1px solid rgba(255, 255, 255, 0.18)",
          "boxShadow": "0 8px 32px 0 rgba(31, 38, 135, 0.07)",
          "padding": "16px 0",
          "transition": "all 0.3s cubic-bezier(0.4, 0, 0.2, 1)"
        },
        "darkMode": {
          "backgroundColor": "rgba(30, 30, 30, 0.7)",
          "borderBottom": "1px solid rgba(255, 255, 255, 0.1)"
        },
        "content": {
          "logo": {
            "type": "Typography",
            "variant": "h6",
            "text": "Ronnie Lutaro",
            "fontWeight": 700,
            "position": "left"
          },
          "navItems": [
            {
              "label": "Home",
              "href": "/",
              "muiComponent": "Button",
              "variant": "text"
            },
            {
              "label": "Writing",
              "href": "/writing",
              "muiComponent": "Button",
              "variant": "text"
            },
            {
              "label": "Contact",
              "href": "/contact",
              "muiComponent": "Button",
              "variant": "text"
            }
          ],
          "actions": {
            "themeToggle": {
              "component": "IconButton",
              "icon": "Brightness4/Brightness7",
              "ariaLabel": "Toggle theme"
            }
          }
        },
        "responsive": {
          "mobile": {
            "component": "Drawer",
            "trigger": "IconButton with MenuIcon",
            "anchor": "right",
            "glassMorphism": true
          }
        }
      },
      {
        "id": "hero-section",
        "component": "Box",
        "height": "100vh",
        "muiComponent": "Box",
        "styling": {
          "background": "mesh-gradient-animated",
          "gradientColors": [
            "#667eea",
            "#764ba2",
            "#f093fb",
            "#4facfe"
          ],
          "animation": "gradient-shift 15s ease infinite",
          "position": "relative",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center",
          "overflow": "hidden"
